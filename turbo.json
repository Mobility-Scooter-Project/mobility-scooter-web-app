{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "outputs": ["dist/**", "build/**"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "compose:up": {
      "persistent": true,
      "cache": false
    },
    "dev": {
      "with": ["compose:up"],
      "persistent": true,
      "cache": false
    },
    "db:up": {
      "dependsOn": ["^db:up"]
    },
    "db:migrate": {
      "with": ["db:up"],
      "dependsOn": ["^db:migrate"]
    },
    "db:seed": {
      "dependsOn": ["^db:seed"]
    },
    "db:studio": {
      "dependsOn": ["^db:studio"]
    },
    "db:drop": {
      "dependsOn": ["^db:drop"]
    },
    "populate-api-key": {
      "dependsOn": ["^populate-api-key"]
    },
    "test": {
      "with": ["compose:up"],
      "cache": false,
      "persistent": false
    }
  },
  "ui": "tui"
}